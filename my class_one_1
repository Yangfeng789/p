#include<stdio.h>
int main()
{
	int arr[5][5]={{4,8,6,34,87},{78,85,6,67,6},{89,78,8,6,8},{55,57,2,8,1},{54,87,3,8,8}};
	int max=arr[0][0],
	min=arr[0][0];
	int i,j;
	for(i=0;i<=4;i++)
	{
	for(j=0;j<=4;j++)
	{
	if(max<arr[i][j])
		max=arr[i][j];
	if(min>arr[i][j])
		min=arr[i][j];
	}
	}
	printf("max is%d,min is%d\n",max,min);
	return 0;
	





}
