#include<stdio.h>
void cfang(int,int);
void cfang(int ds,int cs)
 {
 
 int i;
 int s=1;
 for(i=1;i<=cs;i++)
 {
 s=s*ds;
 }

 printf("%d\n",s);

 }
 
int main()
 {
 cfang(5,5);
 cfang(5,3);
 return 0;
 }
